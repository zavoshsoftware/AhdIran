
@{
		Layout = null;
}
<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<meta name="description" content="Themeforest Template Polo, html template">
	<link rel="canonical" href="@ViewBag.Canonical" />
	<link rel="icon" type="image/png" href="/images/favicon.png">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>@ViewBag.Title</title>
	<link href="/Content/FrontEnd/plugins.css" rel="stylesheet">
	<link href="/Content/FrontEnd/style.css" rel="stylesheet">
	<link href="~/Content/FrontEnd/style-zavosh.css" rel="stylesheet" />
	@RenderSection("head", required: false)
</head>


<body>


<!-- Body Inner -->
<div class="body-inner">

@Html.Partial("FrontEnd/_MenuPartial")
	@RenderBody()
@Html.Partial("FrontEnd/_FooterPartial")

	<div class="whatsappicon moving-div">
		<a href="https://api.whatsapp.com/send?phone=09229157300&text=سلام و وقت بخیر">
			<img src="~/images/whatssapp.png" />
		</a>
	</div>

</div>
<!-- end: Body Inner -->
<!-- Scroll top -->
<a id="scrollTop"><i class="icon-chevron-up"></i><i class="icon-chevron-up"></i></a>
<!--Plugins-->
<script src="/Scripts/FrontEnd/jquery.js"></script>
<script src="/Scripts/FrontEnd/plugins.js"></script>

<!--Template functions-->
<script src="/Scripts/FrontEnd/functions.js"></script>
@RenderSection("scripts", required: false)

</body>

</html>
