
@{
		Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="وب سایت رسمی شرکت عهد ایرانیان، ارائه دهنده خدمات اینترنت پرسرعت و خدمات شبکه">
    <link rel="canonical" href="@ViewBag.Canonical" />
    <link rel="icon" type="image/png" href="/images/favicon.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <link href="~/Content/bootstrap-5.1.0-dist/css/bootstrap.rtl.min.css" rel="stylesheet" />
    <link href="/Content/FrontEnd/plugins.css" rel="stylesheet">
    <link href="/Content/FrontEnd/style.css" rel="stylesheet">
    <link href="~/Content/FrontEnd/style-zavosh.css" rel="stylesheet" />
    @RenderSection("head", required: false)
</head>


<body>

    <div class="body-inner">

        @Html.Partial("FrontEnd/_MenuPartial")
        @RenderBody()
        @Html.Partial("FrontEnd/_FooterPartial")

        <div class="whatsappicon moving-div">
            <a href="https://api.whatsapp.com/send?phone=+989229157400&text=سلام و وقت بخیر">
                <img src="~/images/whatssapp.png" />
            </a>
        </div>

    </div>
    <a id="scrollTop"><i class="icon-chevron-up"></i><i class="icon-chevron-up"></i></a>
    <script src="/Scripts/FrontEnd/jquery.js"></script>
    <script src="~/Content/bootstrap-5.1.0-dist/js/bootstrap.bundle.min.js"></script>
    <script src="/Scripts/FrontEnd/plugins.js"></script>
    <script src="/Scripts/FrontEnd/functions.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
